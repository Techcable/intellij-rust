<idea-plugin package="org.rust.js">
    <!--suppress PluginXmlValidity -->
    <dependencies>
        <plugin id="JavaScript"/>
    </dependencies>

    <extensions defaultExtensionNs="com.intellij">
        <codeInsight.lineMarkerProvider language="Rust" implementationClass="org.rust.js.RsWasmBindgenLineMarkerProvider"/>
    </extensions>

    <extensions defaultExtensionNs="JavaScript.JsonSchema">
        <ProviderFactory implementation="org.rust.toml.schema.RsSchemaProviderFactory"/>
    </extensions>
</idea-plugin>
